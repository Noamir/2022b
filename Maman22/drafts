int validate(char *c)
{
    // check what kind of operation was called

    // parse first command
    int cmd = getCommand(c);
    trimSpaces(c);

    switch (cmd)
    {
    case 1:
        return validateReadMat(c);
    case 2:
        return validateMulMat(c);
    case 3:
        return 0;
    default:
        return -1;
    }
}

int validateReadMat(char *c)
{
    if (expectMat(c) == -1)
    {
        return -1;
    }

    while (1)
    {
        if (expectComma(c) == -1)
        {
            return -1;
        }
        if (expectNumber(c) == -1)
        {
            return -1;
        }
        if (finished(c))
        {
            break;
        }
    }
    return 0;
}

int expectMat(char *c)
{
    return -1;
}




mat_t *get_mat_by_name(char *name)
{
    printf("mat name: %s", name);
    return MAT_A;
}

print_mat_t *to_struct_for_print(char *print_string)
{
    print_mat_t *print_struct;
    print_struct = calloc(1, sizeof(print_mat_t));

    print_struct->action = strtok(print_string, " ");

    printf("The action name is: %s\n", print_struct->action);

    print_struct->mat_name = strtok(NULL, "\n");

    printf("The mat name is: %s\n", print_struct->mat_name);

    return print_struct;
}